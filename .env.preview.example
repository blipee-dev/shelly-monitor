# Preview/Staging Environment Variables Template
# These should be set in Vercel Dashboard for preview deployments

# Application URLs
NEXT_PUBLIC_APP_URL=https://preview.blipee.com

# Supabase Configuration (Staging)
NEXT_PUBLIC_SUPABASE_URL=https://staging-xxxxx.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=staging-anon-key
SUPABASE_SERVICE_ROLE_KEY=staging-service-role-key

# Security Keys (Different from production)
JWT_SECRET=staging-jwt-secret-min-32-chars
SESSION_SECRET=staging-session-secret-min-32-chars
ENCRYPTION_KEY=staging-32-byte-encryption-key
CRON_SECRET=staging-cron-secret-min-32-chars

# AI Provider (Test/Limited Keys)
DEEPSEEK_API_KEY=test-deepseek-api-key
# OPENAI_API_KEY=test-openai-api-key
# ANTHROPIC_API_KEY=test-anthropic-api-key

# Email Configuration (Test)
SENDGRID_API_KEY=test-sendgrid-api-key
SENDGRID_FROM_EMAIL=staging@blipee.com

# Push Notifications (Staging)
NEXT_PUBLIC_VAPID_PUBLIC_KEY=staging-vapid-public-key
VAPID_PRIVATE_KEY=staging-vapid-private-key
VAPID_SUBJECT=mailto:staging@blipee.com

# Feature Flags (Preview/Staging)
NEXT_PUBLIC_ENABLE_PWA=true
NEXT_PUBLIC_ENABLE_ANALYTICS=false
NEXT_PUBLIC_ENABLE_DEBUG=true
NEXT_PUBLIC_ENABLE_DEV_TOOLS=false
NEXT_PUBLIC_ENABLE_EXPORTS=true
NEXT_PUBLIC_SHOW_PREVIEW_BANNER=true

# Feature Flags - Experimental
NEXT_PUBLIC_ENABLE_EXPERIMENTAL_FEATURES=true
NEXT_PUBLIC_ENABLE_BETA_FEATURES=true

# Analytics (Disabled for preview)
# NEXT_PUBLIC_GA_MEASUREMENT_ID=G-STAGING123
# MIXPANEL_TOKEN=test-mixpanel-token

# Error Tracking (Staging)
NEXT_PUBLIC_SENTRY_DSN=https://xxxxx@sentry.io/staging
SENTRY_AUTH_TOKEN=staging-sentry-auth-token
SENTRY_ORG=blipee
SENTRY_PROJECT=blipee-os-staging

# External Services (Test)
# LOGTAIL_TOKEN=test-logtail-token
# DATADOG_API_KEY=test-datadog-api-key

# Development Helpers
NEXT_PUBLIC_MOCK_DATA_ENABLED=true
NEXT_PUBLIC_SHOW_DEV_WARNINGS=true