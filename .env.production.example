# Production Environment Variables Template
# These should be set in Vercel Dashboard for production deployments

# Application URLs
NEXT_PUBLIC_APP_URL=https://app.blipee.com

# Supabase Configuration (Production)
NEXT_PUBLIC_SUPABASE_URL=https://prod-xxxxx.supabase.co
NEXT_PUBLIC_SUPABASE_ANON_KEY=prod-anon-key
SUPABASE_SERVICE_ROLE_KEY=prod-service-role-key

# Security Keys (Use strong, unique values)
JWT_SECRET=production-jwt-secret-min-32-chars
SESSION_SECRET=production-session-secret-min-32-chars
ENCRYPTION_KEY=production-32-byte-encryption-key
CRON_SECRET=production-cron-secret-min-32-chars

# AI Provider (Production Keys)
DEEPSEEK_API_KEY=prod-deepseek-api-key
# OPENAI_API_KEY=prod-openai-api-key
# ANTHROPIC_API_KEY=prod-anthropic-api-key

# Email Configuration (Production)
SENDGRID_API_KEY=prod-sendgrid-api-key
SENDGRID_FROM_EMAIL=notifications@blipee.com

# Push Notifications (Production)
NEXT_PUBLIC_VAPID_PUBLIC_KEY=prod-vapid-public-key
VAPID_PRIVATE_KEY=prod-vapid-private-key
VAPID_SUBJECT=mailto:support@blipee.com

# Feature Flags (Production)
NEXT_PUBLIC_ENABLE_PWA=true
NEXT_PUBLIC_ENABLE_ANALYTICS=true
NEXT_PUBLIC_ENABLE_DEBUG=false
NEXT_PUBLIC_ENABLE_DEV_TOOLS=false
NEXT_PUBLIC_ENABLE_EXPORTS=true
NEXT_PUBLIC_SHOW_PREVIEW_BANNER=false

# Analytics (Production)
NEXT_PUBLIC_GA_MEASUREMENT_ID=G-PROD12345
MIXPANEL_TOKEN=prod-mixpanel-token

# Error Tracking (Production)
NEXT_PUBLIC_SENTRY_DSN=https://xxxxx@sentry.io/xxxxx
SENTRY_AUTH_TOKEN=prod-sentry-auth-token
SENTRY_ORG=blipee
SENTRY_PROJECT=blipee-os-prod

# External Services (Production)
LOGTAIL_TOKEN=prod-logtail-token
# DATADOG_API_KEY=prod-datadog-api-key

# SMS Notifications (Production)
# TWILIO_ACCOUNT_SID=prod-twilio-account-sid
# TWILIO_AUTH_TOKEN=prod-twilio-auth-token
# TWILIO_PHONE_NUMBER=+1234567890

# External Integrations (Production)
# IFTTT_KEY=prod-ifttt-key
# HOME_ASSISTANT_TOKEN=prod-home-assistant-token